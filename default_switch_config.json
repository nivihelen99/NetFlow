{
  "global": {
    "hostname": "MyNetflowSwitch",
    "log_level": "DEBUG"
  },
  "fdb": {
    "aging_time_seconds": 600
  },
  "stp": {
    "bridge_priority": 32768,
    "hello_time_seconds": 2,
    "max_age_seconds": 20,
    "forward_delay_seconds": 15
  },
  "ports": [
    { "id": 0, "admin_up": true, "speed_mbps": 1000, "vlan_mode": "access", "native_vlan": 10 },
    { "id": 1, "admin_up": true, "speed_mbps": 1000, "vlan_mode": "trunk", "allowed_vlans": [10, 20, 30], "native_vlan": 1 }
  ]
}
